        ]
    },
    "Angular Flex Item Offset": {
        "prefix": "fx-item-offset",
        "description": "Flex Layout Item with fxFlexOffset",
        "types": "typescript, html",
        "body": [
            "<div fxFlex=\"${1:value}\" fxFlexOffset=\"${2:offset}px\">",
            "\t$0",
            "</div>"
        ]
    },
