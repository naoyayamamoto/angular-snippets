{
    "Material Table": {
        "prefix": "m-table",
        "description": "Material Table",
        "types": "typescript, html",
        "body": [
            "<mat-table #table [dataSource]=\"${1:dataSource}\">",
            "\t<ng-container cdkColumnDef=\"${2:column}\">",
            "\t\t<mat-header-cell *cdkHeaderCellDef>${3:header}</mat-header-cell>",
            "\t\t<mat-cell *cdkCellDef=\"let row\">{{row.${2:column}}}</mat-cell>",
            "\t</ng-container>",
            "</mat-table>$0"
        ]
    },
    "Material Table Column": {
        "prefix": "m-table-column",
        "description": "Material Table Column",
        "types": "typescript, html",
        "body": [
            "<ng-container cdkColumnDef=\"${1:column}\">",
            "\t<mat-header-cell *cdkHeaderCellDef>${2:header}</mat-header-cell>",
            "\t<mat-cell *cdkCellDef=\"let row\">{{row.${1:column}}}</mat-cell>",
            "</ng-container>$0"
        ]
    }
}