{
    "Material List": {
        "prefix": "m-list",
        "description": "Material List",
        "types": "typescript, html",
        "body": [
            "<mat-list>",
            "\t<mat-list-item>${1:item}</mat-list-item>$1",
            "</mat-list>$0"
        ]
    },
    "Material List ngFor": {
        "prefix": "m-list-ngfor",
        "description": "Material List ngFor",
        "types": "typescript, html",
        "body": [
            "<mat-list>",
            "\t<mat-list-item *ngFor=\"let ${1:item} of ${2:items}\">{{ ${1:item} }}</mat-list-item>$1",
            "</mat-list>$0"
        ]
    },
    "Material List Item": {
        "prefix": "m-list-item",
        "description": "Material List Item",
        "types": "typescript, html",
        "body": [
            "<mat-list-item>${1:item}</mat-list-item>$0"
        ]
    }
}