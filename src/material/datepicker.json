{
    "Material Datepicker": {
        "prefix": "m-datepicker",
        "description": "Material Datepicker",
        "types": "typescript, html",
        "body": [
            "<mat-form-field>",
            "\t<input matInput [matDatepicker]=\"${1:picker}\" placeholder=\"${2:title}\">",
            "\t<mat-datepicker-toggle matSuffix [for]=\"${1:picker}\"></mat-datepicker-toggle>",
            "\t<button matSuffix [matDatepickerToggle]=\"${1:picker}\"></button>",
            "\t<mat-datepicker #${1:picker}></mat-datepicker>",
            "</mat-form-field>",
            "$0"
        ]
    }
}